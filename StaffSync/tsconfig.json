{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "esnext",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "files": [
    "expo-env.d.ts"
  ],
  "resolve": {
    "extensions": [
      ".tsx",
      ".ts",
      ".js",
      ".json",
      ".png"
    ]
  }
}
